@article{xiaohaiHygrothermalSimulationEvaluation,
  title = {Hygrothermal Simulation and Evaluation of Frost Risk of Masonry Walls Subjected to inside Insulation Retrofitting},
  author = {Xiaohai, {\relax ZHOU} and Dominique, {\relax DEROME} and Jan, {\relax CARMELIET}},
  abstract = {For historical buildings with a worth-preserving facade, interior wall insulation can be the only possible solution to improve the building energy efficiency. However, the application of an interior insulation layer changes significantly the hygrothermal performance of the building envelope. For masonry walls, such intervention may lead to freeze-thaw damage of the brickwork. A hygrothermal model is developed in this study which not only accounts for moisture and heat transport in porous medium but also tracks the occurrence of freezing and thawing in function of pore size distribution and determines the ice content. The numerical model results are compared with a porous medium freezing experiment and good agreement is found. Compared to the traditional method that uses the number of zero crossings on a Celsius scale as the number of freeze-thaw cycles, we propose a method that uses the number of actual ice growth and melt cycles as an indicator more accurately accounting for the freeze-thaw process. To demonstrate this approach, we perform simulations of base and retrofitted masonry walls using two Swiss climatic conditions. The study shows the increase of freeze-thaw cycles and ice content after internal retrofitting in both climates. However, the increase of frost risk is different among the two climatic conditions.},
  langid = {english},
  file = {C\:\\Users\\Andreas Sarkany\\Zotero\\storage\\77YUETXQ\\Xiaohai et al. - Hygrothermal simulation and evaluation of frost ri.pdf}
}
