@article{taoukilMoistureContentInfluence2013,
  title = {Moisture Content Influence on the Thermal Conductivity and Diffusivity of Wood\textendash Concrete Composite},
  author = {Taoukil, Driss and {El bouardi}, Abdelmajid and Sick, Friedrich and Mimet, Abdelaziz and Ezbakhe, Hassan and Ajzoul, Taib},
  year = {2013},
  month = nov,
  journal = {Construction and Building Materials},
  volume = {48},
  pages = {104--115},
  issn = {0950-0618},
  doi = {10.1016/j.conbuildmat.2013.06.067},
  abstract = {The aim of the work reported in this paper is to determine the influence of moisture content on the thermal proprieties of wood\textendash concrete composite, i.e. thermal conductivity and thermal diffusivity. The wood shavings have been incorporated, without any preliminary treatment, into a sand\textendash cement mixture. Five formulations containing different percentage of shavings have been prepared and have been examined herein. The experimental results show that the lightening of concrete by wood shavings increases the thermal insulation capacity by decreasing the thermal conductivity and diffusivity; however these proprieties are strongly dependent on water content. The thermal conductivity increases rapidly with water content. Its experimental evolution with water content was confirmed by the comparison with three theoretical models. The values of the thermal diffusivity depend on the used counting model. The results stemming from three most used models are compared between them, and they show that in general the thermal diffusivity presents a maximum corresponding to a water content value Wm.},
  langid = {english},
  keywords = {Hygrothermal effect,Moisture content,Thermal conductivity,Thermal diffusivity,Wood concrete},
  file = {C\:\\Users\\Andreas Sarkany\\Zotero\\storage\\E8EVU7M4\\Taoukil et al. - 2013 - Moisture content influence on the thermal conducti.pdf;C\:\\Users\\Andreas Sarkany\\Zotero\\storage\\IKZ69IF4\\S0950061813005850.html}
}

